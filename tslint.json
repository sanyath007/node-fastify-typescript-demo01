{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [false],
        "import-name": [false],
        "variable-name": [false],
        "function-name": [false],
        "no-unused-expression": [false],
        "indent": [false],
        "no-console": [false],
        "no-reference": [false],
        "no-var-requires": [false],
        "prefer-template": [false]
    },
    "rulesDirectory": []
}
